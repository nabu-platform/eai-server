			<artifactId>channels-resources</artifactId>
			<version>1.1-SNAPSHOT</version>
		</dependency>

		<!-- Glue dependencies -->
		<dependency>
			<groupId>be.nabu.glue</groupId>
			<artifactId>glue</artifactId>
			<version>1.0-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>be.nabu.glue</groupId>
			<artifactId>glue-types</artifactId>
